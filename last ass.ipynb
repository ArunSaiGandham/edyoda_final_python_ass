    "class pow:\n",
    "    def __init__(self):\n",
    "        self.val = int(input(\"Enter the value\"))\n",
    "        self.mul = int(input(\"Enter the power value\"))\n",
    "       \n",
    "    \n",
    "    def power(self):\n",
    "         return self.val ** self.mul\n",
    "check = pow()\n",
    "\n",
    "print(check.power())"
